autonum 1,1
close all
open #1: name "SER"
open #2: name "ZPL"
sub recibeDato
input #1: A$
let fecha$ = A$(37:44)
let hora$ = A$(24:31)
let resultado$ = A$(57:58)
let resultado$ = rtrim$(resultado$)
sub imprimeDato
print #2: "^xa^fo50,50^a0n,50,50^fd"&fecha$&"^fs^xz"
print #2: "^xa^fo50,50^a0n,50,50^fd"&hora$&"^fs^xz"
print #2: "^xa^fo50,50^a0n,50,50^fd"&resultado$&"^fs^xz"
gosub recibeDato